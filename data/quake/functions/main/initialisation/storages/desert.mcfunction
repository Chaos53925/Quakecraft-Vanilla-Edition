# beforethegame
data merge storage quake:desert {exist: true, team: "desert", teamcolor: "red"}
data merge storage quake:desert {votetag: "desertvote", votescoreboard: "desertvote", votename: "#desertvote", votescore: "desertvoting"}
data merge storage quake:desert {bar: desertbar, barname: "#desertbar", teamlobby: "#desertlobby", seconds: "#desertsek", minutes: "#desertmin", quickplayers: "#desertquick", teamname: "#desert"}
# game
data merge storage quake:desert {teamkills: "DesertKills"}
data merge storage quake:desert {teamprogress: "#desertprogress"}
data merge storage quake:desert {teamprogressbar: "desertprogress", teambarname: "Desert"}
# place
data merge storage quake:desert {placescore: "desertPlace", teamtemp: "desertTemp"}
# Join
data merge storage quake:desert {mapname: "Desert", tpcoords: "-1384 42.3 246 180.0 0.0", spawncoords: "-1384 42.3 246 180.0", jointrigger: "desertjoin"}
